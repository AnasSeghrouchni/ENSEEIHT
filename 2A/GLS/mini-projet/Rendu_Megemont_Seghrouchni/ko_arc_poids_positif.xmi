<?xml version="1.0" encoding="UTF-8"?>
<petrinet:ReseauPetri
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:petrinet="http://petrinet"
    xsi:schemaLocation="http://petrinet PetriNet.ecore"
    name="Exemple">
  <noeud xsi:type="petrinet:Place"
      entrant="//@transition.0"
      name="P1"
      nbJeton="1"/>
  <noeud xsi:type="petrinet:Transition"
      entrant="//@transition.1"
      sortants="//@transition.0"
      name="T1"/>
  <noeud xsi:type="petrinet:Place"
      sortants="//@transition.1"
      name="P2"/>
  <transition
      cible="//@noeud.1"
      source="//@noeud.0"/>
  <transition
      poids="1"
      cible="//@noeud.2"
      source="//@noeud.1"/>
</petrinet:ReseauPetri>
