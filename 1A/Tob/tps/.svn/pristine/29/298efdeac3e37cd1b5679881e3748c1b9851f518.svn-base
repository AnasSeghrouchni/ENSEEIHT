package allumettes;

public class StratRapide implements Strategie {

	/** Renvoie le nombre d'allumettes pris selon la stratégie rapide,
	 *  consistant à prendre le maximum d'allumettes possibles.
	 *  @param Jeu le jeu en cours.
	 */
	public int getPrise(Jeu jeu) {
		if (jeu.getNombreAllumettes()>=Jeu.PRISE_MAX) {
			return Jeu.PRISE_MAX;
		}
		else {return jeu.getNombreAllumettes();}
	}

}
