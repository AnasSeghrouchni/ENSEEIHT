package body compression is

    procedure Cree_Huffman(Nom_Fichier : in String; A: out T_Arbre) is
        S:T_Chaine;
    begin
        Frequence(Sda,Nom_Fichier,File);
        Construction_Arbre(Sda,A);
    end;

    procedure Construsction_table(Arbre:T_Arbre; Sda:T_LCA; Chaine:Unbounded_String) is
    begin
        if Est_feuille(arbre) then
            Enregistrer(Sda, Arbre.all.Cle, Chaine);
        else
            Construction_table(Arbre.all.Gauche,Sda,Chaine & "0");
            Construction_table(Arbre.all.Droite,Sda,Chaine & "1");
        end if;
    end Construction_table;

