package body compression is

    procedure Cree_Huffman(Nom_Fichier : in String; A: out T_Arbre) is
        S:T_Chaine;
    begin
        Frequence(Sda,Nom_Fichier,File);
        Construction_Arbre(Sda,A);
    end;


    Procedure Affiche_Couple(Octet : T_Octet, Str : Unbounded_String) is
    Begin
      Put("'");
      if T_Octet'Image(Octet)=10 then
         Put("\n");
      elsif T_Octet'Image(Octet)=-1 then
         Put("\$");
      elsif T_Octet'Image(Octet)=32 then
         Put("\" \"");
      else
         Put(Character'Val(Octet));
      end if;
        Put("'");
        Put("-->");
        Put(To_String(Str));
        New_Line;
    End Affiche_Couple;
    Procedure Afficher_Table is new Pour_Chaque(Affiche_Couple);



end compression;
